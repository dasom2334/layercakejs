nav#sidebarMenu.col-md-3.col-lg-2.d-md-block.bg-light.sidebar.collapse
  .position-sticky.pt-3
    ul.nav.flex-column
      li.nav-item
        a.nav-link.active(aria-current='page' href='/')
          span(data-feather='home')
          |               Dashboard
      - for (let i in menus)
        li.nav-item
          a.nav-link.active(aria-current='page' href= menus[i]['link'])
            span(data-feather= menus[i]['icon'])
            |               #{menus[i]['name']}